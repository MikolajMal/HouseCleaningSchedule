<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Main colors-->
    <SolidColorBrush x:Key="MainBackgroundBrush" Color="#1E1E1E"/>
    <SolidColorBrush x:Key="ControlBackgroundBrush" Color="#353535"/>
    <SolidColorBrush x:Key="BorderColorBrush" Color="#474747"/>

    <!--Text colors-->
    <SolidColorBrush x:Key="TextBrush" Color="#EDEAEA"/>
    <SolidColorBrush x:Key="SecondaryTextBrush" Color="#7D7D7D"/>
</ResourceDictionary>